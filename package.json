{
  "name": "storybook-blazor",
  "version": "1.0.0",
  "description": "This is the Style Documentation for the OWL.Cultur-Platform",
  "main": "index.js",
  "scripts": {
    "buildBlazor": "dotnet publish -c Release -p:PublishReadyToRun=true -p:PublishTrimmed=true ./storybook-blazor",
    "copyGithubFiles": "copyfiles -a --flat ./gh-pages/* ./storybook-static-gh",
    "build-storybook": "npm run buildBlazor && build-storybook -s ./storybook-blazor/bin/Release/netstandard2.1/publish/wwwroot,./htmlcss-components -o ./storybook-static",
    "build-storybook-gh": "npm run buildBlazor && build-storybook -s ./storybook-blazor/bin/Release/netstandard2.1/publish/wwwroot,./htmlcss-components -o ./storybook-static-gh && npm run copyGithubFiles",
    "deploy-storybook": "storybook-to-ghpages --existing-output-dir=./storybook-static-gh --remote=github --branch=gh-pages",
    "storybook": "npm run buildBlazor && start-storybook -s ./storybook-blazor/bin/Release/netstandard2.1/publish/wwwroot,./htmlcss-components -p 9001",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "aXon GmbH & Universit√§t Paderborn | SI-Lab",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@storybook/addon-a11y": "^5.3.19",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-backgrounds": "^5.3.19",
    "@storybook/addon-centered": "^5.3.19",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-essentials": "^5.3.19",
    "@storybook/addon-events": "^5.3.19",
    "@storybook/addon-jest": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-notes": "^5.3.19",
    "@storybook/addon-options": "^5.3.19",
    "@storybook/addon-storyshots": "^5.3.19",
    "@storybook/addon-storysource": "^5.3.19",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/client-api": "^5.3.19",
    "@storybook/core": "^5.3.19",
    "@storybook/core-events": "^5.3.19",
    "@storybook/html": "^5.3.19",
    "@storybook/source-loader": "^5.3.19",
    "@storybook/storybook-deployer": "^2.8.6",
    "@storybook/theming": "^5.3.19",
    "babel-loader": "^8.1.0",
    "copyfiles": "^2.3.0",
    "eventemitter3": "^4.0.4",
    "format-json": "^1.0.3",
    "global": "^4.4.0"
  },
  "dependencies": {}
}
